{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2021"],
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "build",
    "importHelpers": false
  },"include": ["src"],

  "exclude": ["node_modules", "templates", "src/template","input","output", "common-types.ts", ".*"]
}
